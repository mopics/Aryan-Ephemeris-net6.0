{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch",
            "type": "coreclr",
            "request": "launch",
            "console": "internalConsole",
            "cwd": "${workspaceFolder}/source/AryanEphemeris.Samples",
            "internalConsoleOptions": "openOnSessionStart",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/source/AryanEphemeris.Samples/bin/Debug/netcoreapp2.0/AryanEphemeris.Samples.dll",
            "stopAtEntry": false,
            "logging": {
                "moduleLoad": false
            },
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}